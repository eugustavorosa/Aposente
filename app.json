{
  "expo": {
    "name": "Aposente",
    "slug": "Aposente",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7B2BBE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**"    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aposente",
      "buildNumber": "1.0.2"
    },
    "android": {
      "package": "com.aposente",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
